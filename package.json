{
  "name": "Polytomizator",
  "version": "1.0.1",
  "description": "A small app that helps generate low-poly art.",
  "repository": "StoneT2000/Polytomizator",
  "main": "PolytomizatorApp/app.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite -all",
    "build:win": "electron-packager . --out=dist --asar --overwrite --platform=win32",
    "build:osx": "electron-packager . --out=dist --asar --overwrite --platform=darwin",
    "build:linux": "electron-packager . --out=dist --asar --overwrite --platform=linux"
  },
  "keywords": [
    "poly",
    "art"
  ],
  "author": "StoneT2000",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.9",
    "electron-packager": "^12.0.0"
  }
}
